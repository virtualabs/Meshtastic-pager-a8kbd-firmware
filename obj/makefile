# All Target
all: 
	$(MAKE) --no-print-directory main-build 

main-build: CH582M.elf secondary-outputs

# Tool invocations
CH582M.elf: $(OBJS) $(USER_OBJS)
	@	riscv-none-embed-gcc -march=rv32imac -mabi=ilp32 -mcmodel=medany -msmall-data-limit=8 -mno-save-restore -fmax-errors=20 -Os -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common -g -T "/home/virtualabs/mounriver-studio-projects/CH582M/Ld/Link.ld" -nostartfiles -Xlinker --gc-sections -L"/home/virtualabs/mounriver-studio-projects/CH582M" -L"/home/virtualabs/mounriver-studio-projects/CH582M/StdPeriphDriver" -Xlinker --print-memory-usage -Xlinker --print-memory-usage -Wl,-Map,"CH582M.map" --specs=nano.specs --specs=nosys.specs -o "CH582M.elf" $(OBJS)  $(LIBS)
CH582M.hex: CH582M.elf
	@	riscv-none-embed-objcopy -O ihex "CH582M.elf" "CH582M.hex"
CH582M.lst: CH582M.elf
	@	riscv-none-embed-objdump --source --all-headers --demangle --line-numbers --wide "CH582M.elf" > "CH582M.lst"
CH582M.siz: CH582M.elf
	riscv-none-embed-size --format=berkeley "CH582M.elf"

# Other Targets
clean:
	-$(RM) $(OBJS) $(EXPANDS) $(CALLGRAPH_DOT) $(SECONDARY_FLASH)$(SECONDARY_LIST)$(SECONDARY_SIZE)$(S_DEPS)$(S_UPPER_DEPS)$(ASM_DEPS)$(ASM_UPPER_DEPS)$(C_DEPS) CH582M.elf

secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

